{
	"authors": [
		"Artha"
	],
	"copyright": "Copyright Â© 2024, Artha",
	"dependencies": {
		"fluid": ">=0.0.0"
	},
	"description": "Tree Sitter integration for Fluid's CodeInput",
	"license": "BSL",
	"name": "fluid-tree-sitter",
	"targetPath": "build",
	"sourcePaths": [
		"source/fluid"
	],
	"stringImportPaths": [
		"external"
	],
	"lflags-linux": [
		"-rpath=$$ORIGIN",
		"-Lbuild"
	],
	"lflags-osx": [
		"-rpath",
		"@executable_path/"
	],
	"dflags": [
		"-P-I$PACKAGE_DIR/external/tree-sitter/lib/include"
	],
	"configurations": [
		{
			"name": "default",
			"targetType": "library",
			"sourcePaths": [
				"source/fluid"
			],
			"dependencies": {
				"fluid-tree-sitter:tree-sitter": {
					"path": "."
				}
			}
		},
		{
			"name": "external",
			"libs": [
				"tree-sitter"
			]
		},
		{
			"name": "unittest",
			"sourceFiles": [
				"source/lib_tree_sitter.c"
			],
			"dependencies": {
				"fluid-tree-sitter:tree-sitter": {
					"path": "."
				},
				"fluid-tree-sitter:smaug": {
					"path": "."
				},
				"fluid-tree-sitter:d": {
					"path": "."
				}
			}
		},
		{
			"name": "unittest-external",
			"libs": [
				"tree-sitter"
			],
			"dependencies": {
				"fluid-tree-sitter:smaug": {
					"path": "."
				},
				"fluid-tree-sitter:d": {
					"path": "."
				}
			}
		}
	],
	"subPackages": [
		{
			"name": "tree-sitter",
			"sourcePaths": [],
			"importPaths": [],
			"targetPath": "build",
			"targetType": "dynamicLibrary",
			"configurations": [
				{
					"name": "windows-bin",
					"targetType": "none",
					"platforms": [
						"windows"
					],
					"libs": [
						"tree-sitter"
					],
					"copyFiles-windows": [
						"$PACKAGE_DIR/libraries/windows/tree-sitter.dll"
					]
				},
				{
					"name": "default",
					"dflags": [
						"-betterC",
						"-P-I$PACKAGE_DIR/external/tree-sitter/lib/include",
						"-P-I$PACKAGE_DIR/external/tree-sitter/lib/src"
					],
					"sourceFiles": [
						"source/lib_tree_sitter.c"
					]
				}
			]
		},
		{
			"name": "smaug",
			"sourceFiles": [
				"source/tree_sitter_smaug.c"
			],
			"sourcePaths": [],
			"importPaths": [],
			"targetPath": "build",
			"targetType": "dynamicLibrary",
			"dflags": [
				"-betterC",
				"-P-I$PACKAGE_DIR/external/tree-sitter-smaug/src"
			],
			"libs-windows": [
				"LIBCMT"
			]
		},
		{
			"name": "d",
			"sourceFiles": [
				"source/tree_sitter_d.c",
				"source/tree_sitter_d_scanner.c"
			],
			"sourcePaths": [],
			"importPaths": [],
			"targetPath": "build",
			"targetType": "dynamicLibrary",
			"dflags": [
				"-betterC",
				"-P-I$PACKAGE_DIR/external/tree-sitter-d/src"
			],
			"libs-windows": [
				"LIBCMT"
			]
		}
	]
}